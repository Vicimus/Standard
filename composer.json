{
    "name": "vicimus/standard",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6",
        "squizlabs/php_codesniffer": "~3",
        "escapestudios/symfony2-coding-standard": "~3",
        "phpmd/phpmd": "^2.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "5.6.31"
        }
    }
}
